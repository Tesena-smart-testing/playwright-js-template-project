{
  "name": "playwright-playground",
  "version": "1.0.0",
  "description": "Set of functional/e2e tests using MS Playwright library, to explore its possibilities.",
  "main": "index.js",
  "scripts": {
    "test": "mocha test\\spec\\** --config .mocharc.json",
    "docs-pop": "documentation build test\\pageobjects\\** -f md -o docs\\pageobjects.md",
    "docs-spec": "documentation build test\\spec\\** -f md -o docs\\spec.md --shallow",
    "docs-helpers": "documentation build test\\helpers\\** -f md -o docs\\helpers.md",
    "mochaRunner-all": "node test\\runners\\mochaRunner.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tesena-smart-testing/playwright-js-template-project.git"
  },
  "keywords": [
    "testing",
    "playwright",
    "automation",
    "nodejs"
  ],
  "author": "Radek 'bednaJedna' Bednarik | Tesena",
  "license": "MIT",
  "devDependencies": {
    "@reportportal/agent-js-mocha": "^5.0.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "documentation": "^13.0.2",
    "eslint": "^7.8.1",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "playwright": "^1.4.0",
    "yargs": "^16.0.3"
  }
}
